# Font settings
font.family: sans-serif
font.size: 9
axes.labelsize: 9
axes.titlesize: 9
xtick.labelsize: 9
ytick.labelsize: 9
legend.fontsize: 9

# Figure and axes
figure.dpi: 100
savefig.dpi: 500
axes.linewidth: 0.8
lines.linewidth: 1
grid.linewidth: 0.5

# Ticks
xtick.major.width: 0.8
ytick.major.width: 0.8
xtick.direction: out
ytick.direction: out

# Save settings
savefig.transparent: False
savefig.format: png
